								"exec": [
									"const id_dell = pm.environment.get(\"folder_id_dell\");\r",
									"\r",
									"pm.sendRequest({\r",
									"    url: \"https://api-v2.pandavideo.com.br/folders\",\r",
									"    method: \"GET\",\r",
